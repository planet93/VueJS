        var mas_for_data = [];

        var js_date = ['03.07', '04.03', '01.02'];
        var js_oc = ['android', 'ios', 'microsogt'];

        //Если объявить перед циклом, то всего один объект
        var mas1 = { device: "", geek: "" };

        console.log("Object mas1");
        for (var i = 0; i < js_oc.length; i++) {
            mas1.device = js_oc[i];
