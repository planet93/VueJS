            mas_for_data[i] = mas1;
            console.log(mas1);
        }

        console.log("Object mas2");
        //Если в цикле, то три объекта
        for (var i = 0; i < js_oc.length; i++) {
            var mas2 = { device: "", geek: "" };
            mas2.device = js_oc[i];
            mas2.geek = js_date[i];
            mas_for_data[i] = mas2;
            console.log(mas2);
